{
  "name": "@edmp/back",
  "version": "1.101.8",
  "description": "Node.js backend for ownily app",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "start": "yarn build && cd ./dist/ && node --trace-warnings --unhandled-rejections=strict index.js",
    "start:dev": "cross-env NODE_CONFIG_DIR=./src/config NODE_ENV=dev nodemon",
    "start:docker:dev": "docker run -d --rm --name redis -p 6379:6379 redis:6.0.13-alpine && docker run -d --rm --name mongodb -p 27017:27017 mongo:4.0.3 && docker run -d --rm --name mongodb-express -p 8081:8081 --link mongodb:mongo mongo-express",
    "stop:docker:dev": "docker rm -f -v redis mongodb-express mongodb",
    "cp:resources": "ncp ./src/resources ./dist/resources",
    "clean": "rm -rf ./dist/* && rm -rf tsconfig.tsbuildinfo",
    "prebuild": "yarn clean",
    "build": "tsc --build tsconfig.json && yarn cp:resources",
    "build:dev": "(cd ../api && yarn build:dev) && yarn prebuild && tsc --build && yarn cp:resources"
  },
  "dependencies": {
    "@casl/ability": "6.3.1",
    "@edmp/api": "^1.0.0",
    "ajv": "8.11.2",
    "ajv-bsontype": "1.0.7",
    "ajv-formats": "2.1.1",
    "aws-sdk": "2.1235.0",
    "axios": "1.1.3",
    "bcryptjs": "2.4.3",
    "config": "3.3.1",
    "connect": "3.7.0",
    "cookie": "0.5.0",
    "cron": "2.1.0",
    "decimal.js-light": "2.5.1",
    "device-detector-js": "3.0.3",
    "fast-levenshtein": "3.0.0",
    "fastest-validator": "1.15.0",
    "helmet": "6.0.0",
    "ioredis": "4.28.5",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "make-promises-safe": "5.1.0",
    "module-alias": "2.2.2",
    "moleculer": "0.14.24",
    "moleculer-io": "2.1.0",
    "moleculer-web": "0.10.4",
    "moment": "2.29.4",
    "moment-timezone": "0.5.38",
    "mongoose": "5.13.9",
    "mongoose-paginate-v2": "1.7.0",
    "multiparty": "4.2.3",
    "mustache": "4.2.0",
    "nodemailer": "6.8.0",
    "nodemailer-markdown": "1.0.3",
    "pino": "8.6.1",
    "pino-pretty": "9.1.1",
    "polka": "0.5.2",
    "prom-client": "14.0.1",
    "redlock": "4.2.0",
    "//": "## Do not remove : saslprep warning when using MongoClient.connect()",
    "saslprep": "1.0.3",
    "shortid": "2.2.16",
    "stripe": "10.14.0",
    "superagent-jsonparse": "1.0.0",
    "swagger-stats": "0.99.4",
    "swagger-ui-dist": "4.14.3",
    "ulid": "2.3.0",
    "webpack-merge": "5.8.0",
    "xlsx-js-style": "1.2.0"
  },
  "devDependencies": {
    "@types/ajv-bsontype": "1.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/config": "3.3.0",
    "@types/connect": "3.4.35",
    "@types/cookie": "0.5.0",
    "@types/cron": "2.0.0",
    "@types/fast-levenshtein": "0.0.2",
    "@types/jest": "^29.2.1",
    "@types/json2csv": "5.0.3",
    "@types/jsonwebtoken": "8.5.1",
    "@types/lodash": "4.14.186",
    "@types/marked": "^5.0.0",
    "@types/module-alias": "2.0.1",
    "@types/mustache": "4.2.2",
    "@types/node": "16.11.7",
    "@types/nodemailer": "6.4.6",
    "@types/polka": "0.5.2",
    "@types/shortid": "0.0.29",
    "@types/superagent": "4.1.15",
    "@types/swagger-stats": "0.95.7",
    "@types/swagger-ui-dist": "3.30.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.33.0",
    "coveralls": "3.1.0",
    "cross-env": "7.0.2",
    "cson": "7.20.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
